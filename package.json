{
  "name": "selectors-translated",
  "version": "1.0.0",
  "description": "Translates CSS Selectors to plain English",
  "scripts": {
    "clean": "rimraf \"dist/*\" \"lib/*\"",
    "start": "webpack-dev-server --open",
    "test": "mocha \"src/test/*.test.ts\"",
    "test:browser": "mocha-play \"src/test/*.test.browser.ts\" -c webpack.config.js",
    "test:all": "run-s lint typecheck test test:browser",
    "lint": "eslint .",
    "build": "tsc",
    "watch": "npm run build -- -w",
    "typecheck": "tsc --noEmit",
    "bundle": "webpack",
    "prebuild:prod": "npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run bundle"
  },
  "keywords": [
    "selector",
    "selectors translator",
    "selectors translated",
    "selectors explained",
    "selectors explained in english"
  ],
  "author": "Yarden Porat",
  "license": "MIT",
  "dependencies": {
    "@tokey/css-selector-parser": "^0.6.0",
    "tsparticles-engine": "^2.5.2",
    "tsparticles-preset-triangles": "^2.5.3"
  },
  "devDependencies": {
    "@ts-tools/node": "^3.0.1",
    "@ts-tools/webpack-loader": "^3.0.2",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mocha": "^9.2.2",
    "mocha-play": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.20.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
