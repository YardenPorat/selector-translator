    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "chai": "^4.3.4",
    "css-loader": "^6.5.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^9.1.4",
    "mocha-play": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.18.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
